{
    "name": "ethical",
    "version": "0.0.2",
    "description": "The Ethical Framework â€” A fullstack JavaScript framework with a moral obligation to keep it simple and scalable.",
    "main": "index.js",
    "scripts": {
        "test": "NODE_ENV=test nyc --all --reporter=text --reporter=html --reporter=cobertura mocha -t 30000  **/*.spec.js",
        "build": "npm run build-client && npm run transpile",
        "build-client": "rollup --config client/init/rollup.dev.js && rollup --config client/init/rollup.prod.js",
        "prepublish": "npm run build",
        "transpile": "node ./transpile.js"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "async-exit-hook": "^2.0.1",
        "babel-core": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "base-64": "^0.1.0",
        "bunyan": "^1.8.12",
        "chai-spies": "^0.7.1",
        "clone": "^2.1.1",
        "enzyme": "^3.2.0",
        "fs-extra": "^4.0.2",
        "fs-promise": "^2.0.3",
        "glob": "^7.1.2",
        "graphql": "^0.11.7",
        "http-proxy": "^1.16.2",
        "jsdom": "^11.5.1",
        "koa": "^2.4.1",
        "koa-compose": "^4.0.0",
        "lodash": "^4.17.4",
        "mime-types": "^2.1.17",
        "minimatch": "^3.0.4",
        "node-fetch": "^1.7.3",
        "object-extend": "^0.5.0",
        "path-browserify": "0.0.0",
        "prop-types": "^15.6.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2",
        "recursive-readdir-sync": "^1.0.6",
        "recursive-watch": "^1.1.3",
        "redux": "^3.7.2",
        "require-hacker": "^3.0.1",
        "rollup": "^0.52.0",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-commonjs": "^8.2.6",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-uglify": "^2.0.1",
        "server-destroy": "^1.0.1",
        "shallowequal": "^1.0.2",
        "uws": "^9.14.0",
        "vinyl": "^2.1.0"
    },
    "devDependencies": {
        "babel-plugin-istanbul": "^4.1.5",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "chai": "^4.1.2",
        "curl": "^0.1.4",
        "mocha": "^4.0.1",
        "nyc": "^11.3.0"
    },
    "nyc": {
        "require": [
            "babel-register"
        ],
        "sourceMap": false,
        "instrument": false
    },
    "babel": {
        "env": {
            "node": {
                "presets": [
                    "react"
                ],
                "plugins": [
                    [
                        "transform-object-rest-spread",
                        {
                            "useBuiltIns": true
                        }
                    ]
                ],
                "sourceMap": "inline",
                "sourceRoot": "node_modules/ethical"
            },
            "test": {
                "presets": [
                    [
                        "env",
                        {
                            "targets": {
                                "browsers": [
                                    "ie >= 11"
                                ]
                            },
                            "debug": false,
                            "useBuiltIns": true
                        }
                    ],
                    "react"
                ],
                "plugins": [
                    "transform-runtime",
                    [
                        "istanbul",
                        {
                            "include": [
                                "**/*.js"
                            ],
                            "exclude": [
                                "**/*.spec.js"
                            ]
                        }
                    ],
                    [
                        "transform-object-rest-spread",
                        {
                            "useBuiltIns": true
                        }
                    ],
                    "transform-class-properties"
                ],
                "sourceMap": "inline",
                "sourceRoot": "node_modules/ethical"
            }
        }
    }
}
